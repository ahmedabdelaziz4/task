<h1>Directors</h1>

<% if @directors.present? %>
  <ul>
    <% @directors.each do |m| %>
      <li>
        <%= link_to m.name,director_path(m) %>
        &mdash;
        <%= m.year %>
        
      </li>
    <% end %>
  </ul>
<% end %>
<% if current_user.try(:admin?) %>
<%= link_to 'Add director',new_director_path%>
<br/>
<%end %>
<%= link_to 'Back' ,:back %>
<br/>
<%= link_to 'Home Page' ,users_path %>

