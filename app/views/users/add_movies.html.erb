<h1>add movies to your watchlist</h1>

<% if @movies.present? %>
  <ul>
    <% @movies.each do |m| %>
      <li>
        <%=  m.name %>
        &mdash;
        <%= link_to "Add",add_to_watchlist_user_path(current_user, movie_id: m.id) %>
        
      </li>
    <% end %>
  </ul>
<% end %>

<%= link_to "Back to movies",movie_path %>
